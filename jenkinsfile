pipeline {
    agent any 

    stages {
        stage('Checkout Code') {
            steps {
                git branch: 'main', 
                    url: 'https://github.com/Anushagurrala/git-migration.git' 
            }
        }
        stage('Executing shell commands') {
            steps {
                    echo "Executing shell commands.."
            }
        }
        stage('Deploy Nginx Container') {
            steps {
                    sh 'docker run -it --rm -d -p 8080:80 --name web nginx'
            }
        }
    }
}
